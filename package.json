{
  "name": "funddeck",
  "version": "1.0.0",
  "private": false,
  "description": "FundDeck - Professional investor presentation platform for startups and entrepreneurs",
  "scripts": {
    "dev": "next dev",
    "dev:turbopack": "next dev --turbopack",
    "dev:bun": "bun --bun run dev",
    "build": "next build",
    "build:turbopack": "next build --turbopack",
    "build:rsbuild": "rsbuild build",
    "build:check": "next build --no-lint",
    "start": "next start",
    "lint": "exec biome lint src/",
    "lint:fix": "exec biome lint --write src/",
    "lint:fix-unsafe": "exec biome lint --unsafe --write src/",
    "format": "exec biome format --write src/",
    "format:check": "exec biome format src/",
    "check": "exec biome check src/",
    "check:fix": "exec biome check --write src/",
    "typecheck": "tsc --noEmit",
    "validate": "pnpm run typecheck && pnpm run lint && pnpm run format:check",
    "validate:full": "pnpm run validate && pnpm run test:ci && pnpm run build:check",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:browser": "vitest --config vitest.browser.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:ci": "vitest run --coverage",
    "test:watch": "vitest",
    "analyze": "ANALYZE=true pnpm build",
    "analyze:server": "BUNDLE_ANALYZE=server pnpm build",
    "analyze:browser": "BUNDLE_ANALYZE=browser pnpm build",
    "security:audit": "pnpm audit --audit-level high",
    "security:check": "pnpm audit --audit-level moderate",
    "deps:check": "depcheck",
    "deps:unused": "depcheck --ignores='@types/*,eslint-*'",
    "precommit": "lint-staged",
    "clean": "rm -rf .next .turbo node_modules/.cache",
    "clean:all": "rm -rf .next .turbo node_modules",
    "reinstall": "pnpm clean:all && pnpm install",
    "commit": "git add . && git-cz",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "perf:vitals": "pnpm dlx web-vitals-reporter",
    "runtime:bun": "bun install && bun run dev",
    "db:setup": "chmod +x scripts/setup-database.sh && ./scripts/setup-database.sh",
    "db:lint": "supabase db lint",
    "db:status": "supabase status",
    "db:validate": "supabase db lint && supabase status",
    "generate:types": "chmod +x scripts/generate-types.sh && ./scripts/generate-types.sh",
    "types:local": "supabase gen types typescript --local > src/types/database.types.ts",
    "types:remote": "supabase gen types typescript --project-id djaxmnjxeccinajmrzvt > src/types/database.types.ts",
    "types:verify": "chmod +x scripts/verify-types.sh && ./scripts/verify-types.sh",
    "types:check": "pnpm run types:verify && pnpm run typecheck",
    "create-admin-users": "node scripts/create-admin-users.js",
    "test:supabase": "node scripts/test-supabase-connection.js",
    "setup:env": "node ./scripts/setup-env.js",
    "db:monitor": "node scripts/monitor-optimization.js",
    "db:monitor:watch": "watch -n 300 'node scripts/monitor-optimization.js'",
    "db:optimize:validate": "node scripts/monitor-optimization.js && echo '‚úÖ Database optimization validation complete'",
    "perf:audit": "pnpm run perf:lighthouse && pnpm run perf:bundle",
    "perf:monitor": "pnpm dlx clinic doctor -- node server.js",
    "upgrade:next": "npx @next/codemod@canary upgrade latest",
    "build:docker": "docker build -t myroomie-platform .",
    "start:docker": "docker run -p 3000:3000 myroomie-platform",
    "auth:audit": "node scripts/auth-audit.js",
    "auth:check": "pnpm auth:audit"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.42.2",
    "@clerk/elements": "^0.23.55",
    "@clerk/nextjs": "^6.31.2",
    "@clerk/shared": "^3.21.1",
    "@conform-to/react": "^1.8.2",
    "@conform-to/zod": "^1.8.2",
    "@google-cloud/local-auth": "3.0.1",
    "@hookform/resolvers": "^5.2.1",
    "@next/env": "canary",
    "@next/third-parties": "canary",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.85.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/og": "^0.8.5",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "exceljs": "^4.4.0",
    "googleapis": "^156.0.0",
    "input-otp": "^1.4.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.540.0",
    "next": "canary",
    "next-safe-action": "^8.0.10",
    "next-themes": "1.0.0-beta.0",
    "papaparse": "^5.5.3",
    "prop-types": "^15.8.1",
    "react": "19.1.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-joyride": "^2.9.3",
    "react-leaflet": "^5.0.0",
    "react-resizable-panels": "^3.0.4",
    "react-simple-maps": "^3.0.0",
    "recharts": "^3.1.2",
    "schema-dts": "^1.1.5",
    "shadcn": "^2.10.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "vaul": "^1.1.2",
    "web-push": "^3.6.7",
    "xlsx": "^0.18.5",
    "zod": "^4.0.17",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@next/bundle-analyzer": "canary",
    "@swc/core": "^1.13.3",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24.3.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-simple-maps": "^3.0.6",
    "@types/uuid": "^10.0.0",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "^1.4.7",
    "dotenv": "^17.2.1",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.1",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.2.4",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.4",
    "tw-animate-css": "^1.3.7",
    "typescript": "next",
    "web-vitals": "^5.1.0"
  },
  "engines": {
    "node": ">=22.x",
    "pnpm": ">=10.14.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not android 4.4.3"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc}": [
      "@biomejs/biome check --fix",
      "@biomejs/biome format --write"
    ],
    "*.{css,md,yml,yaml}": [
      "@biomejs/biome format --write"
    ],
    "package.json": [
      "@biomejs/biome format --write"
    ],
    "src/types/database.types.ts": [
      "bash -c 'pnpm run types:verify || true'"
    ],
    "supabase/migrations/*.sql": [
      "bash -c 'echo \"üóÑÔ∏è Validating migration file: $0\" && supabase db lint || true'"
    ],
    "*.{ts,tsx}": [
      "bash -c 'pnpm run typecheck'"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "style",
          "refactor",
          "test",
          "chore",
          "perf",
          "ci",
          "build",
          "db",
          "api",
          "ui",
          "auth",
          "config"
        ]
      ],
      "subject-max-length": [
        2,
        "always",
        72
      ],
      "body-max-line-length": [
        2,
        "always",
        130
      ]
    }
  },
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/src/$1",
    "^server-only$": "<rootDir>/node_modules/server-only/index.js"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(@vercel/analytics|@vercel/speed-insights)/)"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/.next/",
    "<rootDir>/node_modules/"
  ],
  "collectCoverageFrom": [
    "src/**/*.{js,jsx,ts,tsx}",
    "!src/**/*.d.ts",
    "!src/**/*.stories.{js,jsx,ts,tsx}",
    "!src/**/*.test.{js,jsx,ts,tsx}",
    "!src/**/index.{js,jsx,ts,tsx}"
  ],
  "packageManager": "pnpm@10.14.0",
  "turbo": {
    "baseBranch": "origin/main",
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build",
          "typecheck",
          "lint:check"
        ],
        "outputs": [
          ".next/**",
          "!.next/cache/**"
        ]
      },
      "build:turbopack": {
        "dependsOn": [
          "^build:turbopack",
          "typecheck",
          "lint:check"
        ],
        "outputs": [
          ".next/**",
          "!.next/cache/**"
        ]
      },
      "build:check": {
        "dependsOn": [
          "^build:check"
        ],
        "outputs": [
          ".next/**",
          "!.next/cache/**"
        ]
      },
      "lint": {
        "dependsOn": [
          "^lint"
        ]
      },
      "lint:check": {
        "dependsOn": [
          "^lint:check"
        ]
      },
      "typecheck": {
        "dependsOn": [
          "^typecheck"
        ]
      },
      "test": {
        "dependsOn": [
          "^test"
        ]
      },
      "test:ci": {
        "dependsOn": [
          "^test:ci"
        ]
      },
      "dev": {
        "cache": false,
        "persistent": true
      },
      "dev:turbopack": {
        "cache": false,
        "persistent": true
      },
      "validate": {
        "dependsOn": [
          "typecheck",
          "lint:check",
          "format:check"
        ]
      },
      "validate:full": {
        "dependsOn": [
          "validate",
          "test:ci",
          "build:check"
        ]
      },
      "perf:lighthouse": {
        "dependsOn": [
          "build"
        ]
      }
    }
  },
  "pnpm": {
    "overrides": {
      "@types/react": "$@types/react",
      "@types/react-dom": "$@types/react-dom"
    },
    "patchedDependencies": {},
    "allowedDeprecatedVersions": {
      "inflight": "*",
      "glob": "*"
    },
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "@tailwindcss/oxide",
      "@vercel/speed-insights",
      "core-js",
      "core-js-pure",
      "esbuild",
      "msw",
      "oxc-resolver",
      "protobufjs",
      "sharp",
      "supabase",
      "tesseract.js",
      "unrs-resolver"
    ],
    "ignoredBuiltDependencies": []
  }
}
